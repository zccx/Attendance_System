<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fzu.shhtest.domain.Question" table="question"
		catalog="classcall">
		<id name="autoid">
			<column name="autoid" />
			<generator class="native"></generator>
		</id>

		<property name="attendanceDatedate">
			<column name="attendanceDatedate" />
		</property>

		<property name="score">
			<column name="score" />
		</property>

		<property name="ID">
			<column name="ID" />
		</property>
		<property name="courseName">
			<column name="courseName" />
		</property>
	</class>
</hibernate-mapping>
































