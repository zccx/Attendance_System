<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fzu.shhtest.domain.Personnel" table="personnel"
		catalog="classcall">
		<id name="ID">
			<column name="ID" />
		</id>

		<property name="Ppassword">
			<column name="Ppassword" />
		</property>

		<property name="Pname">
			<column name="Pname" />
		</property>


		<property name="major">
			<column name="major" />
		</property>

		<property name="role">
			<column name="role" />
		</property>
		
		<property name="pclass">
			<column name="pclass" />
		</property>
	</class>
</hibernate-mapping>
































